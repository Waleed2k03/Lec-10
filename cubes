#include <iostream>
#include <string>
using namespace std;
int main()
{
    int r;
    cout << "Enter a number to find its cube" << endl;
    cin >> r;
    for (int x = 1; x <= r; x++)
    {
        cout << "\nNumber is " << x << "the cube is";
        int y = x;
            y = y * y * y;
            cout << y;
    }
}
