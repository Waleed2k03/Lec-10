#include <iostream>
#include <string>
using namespace std;
int main()
{
    int sum = 0;
    for (int x = 100; x <= 200; x++)
    {
        if (x % 9 == 0)
        {
            cout << "\nNumber is " << x << endl;
            sum = sum + x;
        }
    }
    cout << "The sum is " << sum << endl;
}
